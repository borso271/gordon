{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "app/api/suggest/index.jsx", "components/Chat/components/AssistantMessage/index.tsx", "components/Chat/utils/copyToClipboard.js", "app/api/openai_summarize/route.ts", "app/hooks/useComparisonChartCanvas.ts", "services/ticker_insight/fetch_ticker_data.js", "services/user_data/fetch_portfolio.js", "services/user_data/fetch_transaction_history.js", "services/user_data/fetch_watchlist.js"  ],
  "exclude": [
    "node_modules"
  ]
}
